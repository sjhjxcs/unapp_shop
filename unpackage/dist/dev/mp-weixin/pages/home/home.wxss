/* 水平间距 */
swiper {
  height: 330rpx;
}
swiper .swiper-item,
swiper image {
  width: 100%;
  height: 100%;
}
.nav_list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  margin: 15px 0;
}
.nav_item {
  height: 128rpx;
  width: 124rpx;
}
.nav_item .nav_image {
  width: 100%;
  height: 100%;
}
.floor_title {
  height: 60rpx;
  width: 100%;
  display: flex;
}
.floor_img_box {
  display: flex;
}
.right_img_box {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  padding-left: 10rpx;
}
.search_box {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 999;
}

